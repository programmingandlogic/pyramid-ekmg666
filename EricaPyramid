/*
 * File: Pyramid.java
 * ------------------
 * This program is a stub for the Pyramid problem, which draws
 * a brick pyramid.
 */

import acm.program.*;
import acm.graphics.*;

public class Pyramid extends GraphicsProgram {

	public void run() {
		double width = getWidth();
		double height = getHeight(); 
		int numB = 0;
		int numL = 0;
		GRect brick = new GRect(30, 12); 
		add(brick);
		brick.setLocation(100,height-12); 
	}

}
